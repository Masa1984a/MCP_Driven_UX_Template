{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "exclude": "**/node_modules",
      "emitTemplate": {
        "filename": "{path}/{name}.types.ts",
        "header": "/** This file is auto-generated by PgTyped - DO NOT EDIT */"
      }
    }
  ],
  "srcDir": "./src/sql",
  "camelCaseColumnNames": true,
  "dbUrl": "${DATABASE_URL}"
}